      "/blog",
      "/blog/from-concept-to-construction-why-designs-get-stripped-down",
      "/blog/how-to-create-backyard-privacy-when-surrounded-by-neighbors",
      "/gallery",
      "/pricing",
      "/about",
      "/business",
      "/terms",
      "/privacy"
    ]
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@qdrant/js-client-rest": "^1.16.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@types/three": "^0.182.0",
    "@xenova/transformers": "^2.17.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "firebase": "^12.7.0",
    "lucide-react": "^0.555.0",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-compare-slider": "^3.1.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.0",
