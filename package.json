{
  "name": "autoscape",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "dev:all": "node server.js & vite",
    "build": "vite build",
    "postbuild": "react-snap",
    "preview": "vite preview",
    "deploy": "firebase deploy",
    "firebase:login": "firebase login"
  },
  "reactSnap": {
    "source": "dist",
    "destination": "dist",
    "fallback": "200.html",
