    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.24",
    "firebase-tools": "^15.0.0",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
